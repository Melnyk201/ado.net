@page
@model WebApplication1.Pages.School.StudentSubjectModel

@{
    ViewData["Title"] = "Index";
}

<h1>StudentSubject</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubject[0].Subject.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubject[0].Student.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubject[0].Student.SecondName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubject[0].Student.MiddleName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.StudentSubject) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.SecondName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.MiddleName)
            </td>
            
        </tr>
}
    </tbody>
</table>
