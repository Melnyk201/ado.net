@page
@model WebApplication1.Pages.School.StudentSubjectRateModel

@{
    ViewData["Title"] = "Index";
}

    <h1>StudentSubjectRate</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubjectRate[0].Rate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubjectRate[0].Month)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubjectRate[0].Subject.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubjectRate[0].Student.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentSubjectRate[0].Student.SecondName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.StudentSubjectRate) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Rate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Subject.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.SecondName)
            </td>
            
        </tr>
}
    </tbody>
</table>
